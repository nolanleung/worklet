{
  "name": "worklet-compose-example",
  "run": {
    "image": "worklet/base:latest",
    "command": ["bash"],
    "composePath": "./test-compose.yml",
    "environment": {
      "DOCKER_DRIVER": "overlay2",
      "DOCKER_TLS_CERTDIR": ""
    },
    "isolation": "full",
    "privileged": true,
    "credentials": {
      "claude": true
    }
  },
  "services": [
    {
      "name": "app",
      "port": 3000,
      "subdomain": "app"
    }
  ]
}