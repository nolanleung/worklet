{
  "fork": {
    "description": "Development fork for testing",
    "exclude": ["node_modules", "*.log", ".DS_Store", "dist", "build"],
    "includeGit": true,
    "name": "worklet"
  },
  "run": {
    "command": ["bash"],
    "environment": {
      "DOCKER_DRIVER": "overlay2",
      "DOCKER_TLS_CERTDIR": ""
    },
    "image": "worklet/base:latest",
    "isolation": "full",
    "privileged": true,
    "volumes": []
  }
}
